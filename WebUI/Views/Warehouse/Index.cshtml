@model IEnumerable<Library.ViewModel.Core.Organizations.WareHouseViewModel>
@{
    ViewBag.Title = "Index";
}
<div class="row">
    <div class="col-xl-12">
        <div class="d-flex justify-content-between pb-1">
            <a href="/Warehouse/Create" class="btn btn-bg" role="button">Add New</a>
            <a href="/Home" class="btn btn-bg top-button-back float-right" role="button">Back</a>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-xl-12">
        <div class="panel panel-default card">
            <div class="card-header Warehouse_tittle">Warehouse Information</div>
            <div class="panel-body card-body">
                <div class="dataTable_wrapper">
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead class="thead-heading">
                            <tr>
                                <th>Sequence</th>
                                <th>Code</th>
                                <th>Name</th>
                                <th>Address</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Sequence)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Code)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Address1)
                                    </td>
                                    <td>
                                        @Html.ActionLink(" ", "Edit", new { id = item.Id }, new { @class = "btn-sm btn-bg fa fa-edit" })
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

