@model IEnumerable<Library.ViewModel.Core.Organizations.BranchViewModel>
@{
    ViewBag.Title = "Index";
}
<div class="row">
    <div class="col-xl-12">
        <div class="d-flex justify-content-end pb-1">
            <a href="/Home" class="btn btn-bg" role="button">Back</a>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-xl-12">
        <div class="card">
            <div class="card-header ProductBasicConfigurationCategory_tittle">Branch Information</div>
            <!-- /.card-header -->
            <div class="card-body">
                <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead class="thead-heading">
                        <tr>
                            <th>Sequence</th>
                            <th>Code</th>
                            <th>Name</th>
                            <th>Company</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Sequence)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Code)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Name)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.CompanyName)
                                </td>
                                <td>
                                    @Html.ActionLink(" ", "Edit", new { id = item.Id }, new { @class = "btn-sm btn-bg fa fa-edit" })
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
            <!-- /.card-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-xl-12 -->
</div>




