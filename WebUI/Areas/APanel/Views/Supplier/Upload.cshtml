@{
    ViewBag.Title = "Create";
}
<div class="row">
    <div class="col-xl-12">
        <div class="">
            <a class="btn btn-default btn-primary add-new" href="~/UploadSample/COUNTRY Wise Products List.xlsx">
                <i class="fa fa-download hidden-xs"></i> Download Sample &nbsp;&nbsp;
            </a>
            <a href="/APanel/Supplier" class="btn btn-bg pull-right mb-2" type="button">Back</a>
        </div>
    </div>
    <!-- /.col-xl-12 -->
</div>
@using (Ajax.BeginForm("Upload", "Supplier", null, new AjaxOptions { HttpMethod = "POST" }, new { @id = "DialogForm", enctype = "multipart/form-data" }))
{
    <!-- /.row -->
    <div class="row ">
        <div class="col-xl-12">
            <div class="card ">
                <div class="card-header User_tittle">Supplier Upload</div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-2">
                            <div class="form-group">
                                <input type="file" id="file" name="file" required="required" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xl-12">
                            <div class="form-group text-right">
                                <div class="col-xl-12">
                                    <button type="submit" class="btn btn-default btn-primary"><i class="fa fa-upload hidden-xs"></i> Upload</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.card-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-xl-12 -->
    </div>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script type="text/javascript">
        $(function () {

        });

    </script>
}